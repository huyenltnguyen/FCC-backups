function largestOfFour(arr) {
  // You can do this!
  var largest = [];
  arr.map(function(subArr) {
    var temp = 0;
    subArr.map(function(num) {
      if (num > temp) {temp = num;}
    });
    largest.push(temp);
  }); 
  return largest;
}

largestOfFour([[4, 5, 1, 3], [13, 27, 18, 26], [32, 35, 37, 39], [1000, 1001, 857, 1]]);
