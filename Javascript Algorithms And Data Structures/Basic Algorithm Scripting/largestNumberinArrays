function largestOfFour(arr) {
  var largest = [];
  for (var i = 0; i < arr.length; i++) {
    var temp = arr[i][0];
    for (var j = 0; j < arr[i].length; j++) {
      if (temp < arr[i][j]) {
        temp = arr[i][j];
      }  
    }
    largest.push(temp);
  }
  return largest;
}

largestOfFour([[17, 23, 25, 12], [25, 7, 34, 48], [4, -10, 18, 21], [-72, -3, -17, -10]]);
